üßæ 6) src/index.js

import React from "react";
import ReactDOM from "react-dom/client";
import App from "./App";
import { PremiumProvider } from "./context/PremiumContext";

const root = ReactDOM.createRoot(document.getElementById("root"));
root.render(
  <React.StrictMode>
    <PremiumProvider>
      <App />
    </PremiumProvider>
  </React.StrictMode>
);


