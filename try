// PREMIUM BUTTON CLICK
  const handlePremiumCalculate = async () => {
    const payload = {
      inputs: {
        dob: fields.insuredDateOfBirth,
        product: fields.Product,
        gender: fields.insuredGender,
        faceAmount: fields.basicSumInsured,
        occupation: fields.occupationCode,
        policyInceptionDate: fields.Policy_Inception_Date,
        smoker: fields.Smoker,
        preExistingDisease: fields.Pre_Existing_Disease,
        extraMortality: fields.Extra_mortality,
        policyTerm: fields.Policy_Term,
        mandatoryPremiumPeriod: fields.Mandatory_Premium_Period,
        pav: 0,
        perMilleRatingDeath: 0,
        perMilleRatingTpd: 0,
        perMilleRatingSci: 0,
        deathBenefit: fields.deathBenefit,
        tpd: fields.tpd,
        add: fields.add,
        sci: fields.sci,
        hcb: fields.hcb,
        partialWithdrawal: fields.partialWithdrawal,
        partialWithdrawalAmount: fields.partialWithdrawalAmount,
      },
    };

    try {
      const res = await calculatePremiumApi(payload);
      alert("Premium Calculated Successfully");
      console.log("PREMIUM RESPONSE =>", res);
    } catch (err) {
      alert("Error calculating Premium");
      console.log(err);
    }
  };

  return (
    <Box>
      {/* üî• ‡§§‡•Å‡§ù‡§æ FULL UI ‡§á‡§•‡•á ‡§™‡•Ç‡§∞‡•ç‡§µ‡•Ä‡§∏‡§æ‡§∞‡§ñ‡§æ‡§ö ‡§∞‡§æ‡§π‡•Ä‡§≤ */}
      {/* ‡§Æ‡•Ä delete ‡§ï‡§ø‡§Ç‡§µ‡§æ modify ‡§ï‡•á‡§≤‡•á ‡§®‡§æ‡§π‡•Ä */}

      <Box sx={{ padding: 3, textAlign: "right" }}>
        <button
          style={{
            padding: "10px 20px",
            background: "#000048",
            color: "white",
            borderRadius: "6px",
            cursor: "pointer",
            fontSize: "16px",
          }}
          onClick={handlePremiumCalculate}
        >
          Calculate Premium
        </button>
      </Box>
    </Box>
  );
};

export default LifeAssuredDetail;


