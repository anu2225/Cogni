‚û° ‡§´‡§æ‡§à‡§≤: src/api/premiumApi.ts

import axios from "./axiosInterceptor";

export const calculatePremiumApi = async (payload: any) => {
  try {
    const response = await axios.post("/api/calc/premium", payload);
    return response.data;
  } catch (error: any) {
    console.error("Premium API Error:", error);
    throw error;
  }
};


