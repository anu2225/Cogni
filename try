import React, { useEffect, useState } from "react";
import { useTranslation } from "react-i18next";

// API ‚Äì Premium Calculation
import { calculatePremiumApi } from "../api/premiumApi";

import {
  YES_NO,
  PRODUCT_OPTIONS,
  EXTRA_MORTALITY_OPTIONS,
  RIDERS,
  MAIN_PLANS,
} from "../constants";

const PlanAndCoverage = ({ fields, setFields, savedTransaction }) => {
  const { t } = useTranslation();

  const [selectedRiders, setSelectedRiders] = useState<string[]>([]);
  const [jsonData, setJsonData] = useState<any>([]);
  const [headers, setHeaders] = useState<any>([]);
  const [OptRiderData, setOptRiderData] = useState<any>([]);
  const [OptHeaders, setOptHeaders] = useState<any>([]);

  const handleFieldChange = (val: string, fieldName: string) => {
    setFields({ ...fields, [fieldName]: val });
  };

  const handlePlanChange = (event: any) => {
    setFields({ ...fields, basicPlan: event.target.value });
  };

  const handleRiderChange = (event: any) => {
    setSelectedRiders(event.target.value);
    setFields({ ...fields, selectedRiders: event.target.value });
  };

  const camelToNormal = (text: string) => {
    return text.replace(/([A-Z])/g, " $1").trim();
  };

  // Populate from saved transaction
  useEffect(() => {
    if (
      savedTransaction &&
      typeof savedTransaction === "object" &&
      Object.keys(savedTransaction).length !== 0
    ) {
      const rider = savedTransaction[0]?.txn_output_json?.outputs?.RiderDetails;
      const optRider =
        savedTransaction[0]?.txn_output_json?.outputs?.OptionalBenefitDetails;

      if (rider?.length) {
        setJsonData(rider);
        setHeaders(Object.keys(rider[0]));
      }

      if (optRider?.length) {
        setOptRiderData(optRider);
        setOptHeaders(Object.keys(optRider[0]));
      }
    }
  }, [savedTransaction]);


