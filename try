import React from "react";
import { Grid, TextField, FormControl, InputLabel, MenuItem, Select } from "@mui/material";

interface Props {
  fields: any;
  onChange: (name: string, value: any) => void;
}

export default function PlanAndCoverage({ fields, onChange }: Props) {
  return (
    <Grid container spacing={3}>

      {/* Product / Option */}
      <Grid item xs={12} sm={6}>
        <FormControl fullWidth variant="standard">
          <InputLabel>Product (Option)</InputLabel>
          <Select
            value={fields._1b_Product || ""}
            onChange={(e) => onChange("_1b_Product", e.target.value)}
          >
            <MenuItem value="Option A">Option A</MenuItem>
            <MenuItem value="Option B">Option B</MenuItem>
          </Select>
        </FormControl>
      </Grid>

      {/* Face Amount */}
      <Grid item xs={12} sm={6}>
        <TextField
          fullWidth
          label="Face Amount"
          variant="standard"
          type="number"
          value={(fields._1d_Face_Amount ?? fields.basicSumInsured) || ""}
          onChange={(e) => onChange("_1d_Face_Amount", Number(e.target.value))}
        />
      </Grid>

      {/* Occupation */}
      <Grid item xs={12} sm={6}>
        <TextField
          fullWidth
          label="Occupation Code"
          variant="standard"
