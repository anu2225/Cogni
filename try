‚≠ê LifeAssuredDetail.tsx ‚Äî PART B (MIDDLE SECTION UI)

‡§π‡•á ‡§ñ‡§æ‡§≤‡•Ä ‡§™‡•á‡§∏‡•ç‡§ü ‡§ï‡§∞ (Part A ‡§®‡§Ç‡§§‡§∞).

// dropdowns
  const [countries, setCountries] = useState([]);
  const [states, setStates] = useState([]);
  const [districts, setDistricts] = useState([]);
  const [evidenceProof, setEvidenceProof] = useState([]);
  const [occupationCode1, setOccupationCode1] = useState([]);
  const [marriageStatus, setMarriageStatus] = useState([]);
  const [familyMemTyp, setFamilyMemTyp] = useState([]);

  useEffect(() => {
    getFamilyMemberType().then((r) => setFamilyMemTyp(r.data));
    getMaritalStatus().then((r) => setMarriageStatus(r.data));
    getOccupationCode().then((r) => setOccupationCode1(r.data));
    getEvidenceProof().then((r) => setEvidenceProof(r.data));
    getCoutries().then((r) => setCountries(r.data));
  }, []);

  useEffect(() => {
    setStates([]);
    getState(fields.insuredNationality).then((r) => setStates(r.data));
  }, [fields.insuredNationality]);

  useEffect(() => {
    setDistricts([]);
    getDistrict(fields.insuredProvince).then((r) => setDistricts(r.data));
  }, [fields.insuredProvince]);

  const [emailErr, setEmailErr] = useState("");
  const [priErr, setPriErr] = useState("");

  const handleEmailChange = (value: string) => {
    setFields({ ...fields, emailAddress: value });

    const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
    setEmailErr(emailRegex.test(value) ? "" : "Invalid Email");
  };

  const handlePhoneChange = (value: string) => {
    setFields({ ...fields, primaryNumber: value });

    const phoneRegex = /^[0-9]{10}$/;
    setPriErr(phoneRegex.test(value) ? "" : "Invalid Number");
  };


