ЁЯЪА STEP 4 тАФ рд╣рд╛ рдХреЛрдб component рдордзреНрдпреЗ рдЯрд╛рдХрд╛:

import React, { useState } from "react";
import { calculatePremium } from "../../api/calculationApi";

export default function PlanAndCoverage() {

  const [form, setForm] = useState({
    age: "",
    sumAssured: "",
    policyTerm: "",
    productCode: ""
  });

  const [premium, setPremium] = useState<number | null>(null);

  const handleChange = (e: any) => {
    setForm({
      ...form,
      [e.target.name]: e.target.value
    });
  };

  const handleCalculate = async () => {
    try {
      const response = await calculatePremium(form);
      setPremium(response.data.premium);
    } catch (err) {
      console.error("Error calculating premium:", err);
    }
  };

  return (
    <div>
      <h3>Premium Calculator</h3>

      <input
        name="age"
        placeholder="Age"
        onChange={handleChange}
      />

      <input
        name="sumAssured"
        placeholder="Sum Assured"
        onChange={handleChange}
      />

      <input
        name="policyTerm"
        placeholder="Policy Term"
        onChange={handleChange}
      />

      <input
        name="productCode"
        placeholder="Product Code"
        onChange={handleChange}
      />

      <button onClick={handleCalculate}>
        Calculate Premium
      </button>

      {premium !== null && (
        <h2>Premium: {premium}</h2>
      )}
    </div>
  );
}


---

