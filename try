    <groupId>org.springframework.boot</groupId>
    <artifactId>spring-boot-starter-webflux</artifactId>
</dependency>

рдмрд╕ рдЭрд╛рд▓рдВ. рдЕрддрд┐рд░рд┐рдХреНрдд netty versions manually рдЬреЛрдбреВ рдирдХреЛрд╕.


