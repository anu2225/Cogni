üßæ 3) src/pages/PlanAndCoverage.jsx

import React, { useContext, useState } from "react";
import { PremiumContext } from "../context/PremiumContext";
import { TextField, Button, MenuItem } from "@mui/material";
import { useNavigate } from "react-router-dom";

export default function PlanAndCoverage() {
  const { setPlanDetails } = useContext(PremiumContext);
  const navigate = useNavigate();

  const today = new Date().toISOString().slice(0, 10);

  const [data, setData] = useState({
    _1b_Product: "",
    _1d_Face_Amount: "",
    _1h_Policy_Inception_Date: today, // SYSDATE default
    _1k_Extra_mortality: "", // e.g., 0.75
    _1p_Policy_Term: "",
    _1r_Mandatory_Premium_Period: ""
  });

  const handleNext = () => {
    if (!data._1b_Product || !data._1d_Face_Amount) {
      alert("Product and Face Amount required");
      return;
    }
    setPlanDetails(data);
    navigate("/premium");
  };

  return (
    <div style={{ maxWidth: 520, margin: "24px auto", padding: 12 }}>
      <h2>Plan & Coverage</h2>

      <TextField
        select
        label="Product"
        fullWidth
        value={data._1b_Product}
        onChange={(e) => setData({ ...data, _1b_Product: e.target.value })}
        sx={{ mt: 2 }}
      >
        <MenuItem value="Option A">Option A</MenuItem>
        <MenuItem value="Option B">Option B</MenuItem>
      </TextField>

      <TextField
        label="Face Amount"
        fullWidth
        type="number"
        value={data._1d_Face_Amount}
        onChange={(e) => setData({ ...data, _1d_Face_Amount: e.target.value })}
        sx={{ mt: 2 }}
      />

      <TextField
        label="Policy Term"
        fullWidth
        value={data._1p_Policy_Term}
        onChange={(e) => setData({ ...data, _1p_Policy_Term: e.target.value })}
        sx={{ mt: 2 }}
      />

      <TextField
        label="Mandatory Premium Period"
        fullWidth
        value={data._1r_Mandatory_Premium_Period}
        onChange={(e) => setData({ ...data, _1r_Mandatory_Premium_Period: e.target.value })}
        sx={{ mt: 2 }}
      />

      <TextField
        select
        label="Extra Mortality"
        fullWidth
        value={data._1k_Extra_mortality}
        onChange={(e) => setData({ ...data, _1k_Extra_mortality: e.target.value })}
        sx={{ mt: 2 }}
      >
        {[0,0.25,0.5,0.75,1,2,5].map(val => <MenuItem key={val} value={val}>{val}</MenuItem>)}
      </TextField>

      <TextField
        label="Policy Inception Date"
        type="date"
        fullWidth
        InputLabelProps={{ shrink: true }}
        value={data._1h_Policy_Inception_Date}
        onChange={(e) => setData({ ...data, _1h_Policy_Inception_Date: e.target.value })}
        sx={{ mt: 2 }}
      />

      <Button variant="contained" fullWidth sx={{ mt: 3 }} onClick={handleNext}>
        Get Premium
      </Button>
    </div>
  );
}


---

üßæ 4) src/pages/PremiumPage.jsx

import React, { useContext, useState } from "react";
import { PremiumContext } from "../context/PremiumContext";
import { Button, CircularProgress } from "@mui/material";

export default function PremiumPage() {
  const { lifeDetails, planDetails } = useContext(PremiumContext);
  const [premium, setPremium] = useState(null);
  const [loading, setLoading] = useState(false);
  const [rawResponse, setRawResponse] = useState(null);

  const buildPayload = () => {
    return {
      request_data: {
        inputs: {
          // combine pages
          ...lifeDetails,
          ...planDetails,

          // hardcoded values required
          _1zb_PAV: 0,
          _1zc_per_mille_rating_death: 1,
          _1zd_per_mille_rating_TPD: 1,
          _1ze_per_mille_rating_SCI: 1,

          _2a_Death_Benefit: "Y",
          _2b_Total_and_Permanent_Disability_TPD: "Y",
          _2c_Accidental_Death_and_Dismemberment_ADD: "Y",
          _2d_Staged_Critical_Illness_SCI: "Y",
          _2e_Hospital_Cash_Benefit_HCB: "Y",

          _3a_Partial_Withdrawl: "Y",
          _3b_Partial_Withdrawl_Amount: 5000
        }
      },
      request_meta: {
        version_id: "57e38b87-052c-4fb1-a6c5-76bd1567df2d",
        call_purpose: "Spark - API Tester",
        source_system: "SPARK"
      }
    };
  };

  const getPremium = async () => {
    setLoading(true);
    setPremium(null);
    try {
      const url = process.env.REACT_APP_COHERENT_URL || "YOUR_COHERENT_URL";
      const token = process.env.REACT_APP_COHERENT_TOKEN || "YOUR_TOKEN";

      const payload = buildPayload();

      const res = await fetch(url, {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify(payload)
      });

      const data = await res.json();
      setRawResponse(data);

      // Extract premium robustly (depends on API shape)
      // Try common places: data.output.Premium, data.premium, data.result.premium
      const extracted =
        data?.output?.Premium ??
        data?.premium ??
        data?.result?.premium ??
        data?.response?.premium ??
        null;

      setPremium(extracted ?? JSON.stringify(data).slice(0, 500)); // fallback show partial raw
    } catch (err) {
      setPremium("Error: " + err.message);
    } finally {
      setLoading(false);
    }
  };

  return (
    <div style={{ maxWidth: 520, margin: "24px auto", padding: 12 }}>
      <h2>Premium Calculation</h2>

      <Button variant="contained" fullWidth onClick={getPremium} disabled={loading}>
        {loading ? <CircularProgress size={20} /> : "Calculate Premium"}
      </Button>

      {premium && (
        <div style={{ marginTop: 20 }}>
          <h3>Premium:</h3>
          <pre style={{ whiteSpace: "pre-wrap" }}>{String(premium)}</pre>
        </div>
      )}

      {rawResponse && (
        <details style={{ marginTop: 12 }}>
          <summary>Show full raw response</summary>
          <pre style={{ maxHeight: 300, overflow: "auto" }}>
            {JSON.stringify(rawResponse, null, 2)}
          </pre>
        </details>
      )}
    </div>
  );
}


---

üßæ 5) src/App.js

import React from "react";
import { BrowserRouter, Routes, Route } from "react-router-dom";
import LifeAssuredDetails from "./pages/LifeAssuredDetails";
import PlanAndCoverage from "./pages/PlanAndCoverage";
import PremiumPage from "./pages/PremiumPage";

export default function App() {
  return (
    <BrowserRouter>
      <Routes>
        <Route path="/" element={<LifeAssuredDetails />} />
        <Route path="/plan" element={<PlanAndCoverage />} />
        <Route path="/premium" element={<PremiumPage />} />
      </Routes>
    </BrowserRouter>
  );
}


