import { apiClient } from "./axiosInterceptor";

export const calculatePremium = (data: any) => {
    return apiClient.post("/api/calc/premium", data);
};

рдмрд╕реНрд╕!
рдЗрдереЗ backend рдЪреЗ URL рддреБрдЭреНрдпрд╛ axiosInterceptor рдордзреНрдпреЗ рдЖрдзреАрдЪ рд╕реЗрдЯ рдЕрд╕реЗрд▓.
