üìå Create a new file:

src/components/PremiumCalculationParent.jsx

import React, { useState } from "react";
import LifeAssuredDetail from "./LifeAssuredDetails";
import PlanAndCoverages from "./PlanAndCoverages";
import { Box, Button, CircularProgress, Snackbar, Alert } from "@mui/material";

export default function PremiumCalculationParent() {
  const [fields, setFields] = useState({
    insuredNationality: "",
    insuredEvidenceOfProof: "",
    insuredIdNumber: "",
    customerIdEbao: "",
    insuredIdExpiryDate: "",
    insuredTitle: "",
    insuredFirstName: "",
    insuredSurName: "",
    insuredDateOfBirth: "",
    insuredGender: "",
    insuredAge: "",
    insuredProvince: "",
    insuredDistrict: "",
    imgURL: "",
    workplaceAddressOptn: "",
    currentAddressOptn: "",
    insContactAddressIndicator: "",
    motorcycleOptn: "",
    regAddrNum: "",
    regAddrVilla: "",
    regAddrMooNum: "",
    regAddrAlley: "",
    regAddrStrNum: "",
    regAddrSubDist: "",
    regAddrZipCode: "",
    regFullAddr: "",
    familyMemberType: "",
    insuredMritalStatus: "",
    workCompanyName: "",
    workAddrNum: "",
    workAddrVilla: "",
    workAddrMooNum: "",
    workAddrAlley: "",
    workAddrStrNum: "",
    insuredWorkProvince: "",
    insuredWorkDistrict: "",
    workAddrSubDist: "",
    workAddrZipCode: "",
    workFullAddress: "",
    cRegAddNum: "",
    cRegAddVilla: "",
    cRegAddMoo: "",
    cRegAddAlley: "",
    cRegAddStrNum: "",
    cRegAddProvince: "",
    cRegAddDist: "",
    cRegAddSubDis: "",
    cRegAddZipCode: "",
    contactFullAddr: "",
    primaryNumber: "",
    homeNumber: "",
    workPhoneNumber: "",
    emailAddress: "",
    occupationCode: "",
    positionHeld: "",
    occupationDetail: "",
    businessDetail: "",
    annualIncome: "",
    otherOccupation: "",

    // ------- Plan & Coverage Fields -------
    PolicyTerm: "",
    SumAssured: "",
    PremiumPaymentTerm: "",
    PaymentFrequency: "",
    Rider_Accidental: "",
    Rider_CriticalIllness: "",
    Rider_HospitalCash: "",
  });

  const [loading, setLoading] = useState(false);
  const [premium, setPremium] = useState(null);
  const [open, setOpen] = useState(false);

  const calculatePremium = async () => {
    setLoading(true);

    try {
      const requestBody = {
        LifeAssured: {
          FirstName: fields.insuredFirstName,
          LastName: fields.insuredSurName,
          Age: fields.insuredAge,
          DOB: fields.insuredDateOfBirth,
          Gender: fields.insuredGender,
          Nationality: fields.insuredNationality,
        },
        Address: {
          Province: fields.insuredProvince,
          District: fields.insuredDistrict,
          ZipCode: fields.regAddrZipCode,
        },
        Plan: {
          PolicyTerm: fields.PolicyTerm,
          SumAssured: fields.SumAssured,
          PPT: fields.PremiumPaymentTerm,
          Frequency: fields.PaymentFrequency,
          Rider_Accidental: fields.Rider_Accidental,
          Rider_CriticalIllness: fields.Rider_CriticalIllness,
          Rider_HospitalCash: fields.Rider_HospitalCash,
        },
      };

      const response = await fetch(
        "https://excel.uat.us.coherent.global/cognizant/api/v3/folders/Sanjana_Test/services/Universal-Life-product-TestPrd/execute",
        {
          method: "POST",
          headers: {
            Authorization: `Bearer YOUR_TOKEN_HERE`,
            "Content-Type": "application/json",
          },
          body: JSON.stringify({ requestBody }),
        }
      );

      const data = await response.json();
      setPremium(data?.Premium || "No premium returned");
      setOpen(true);
    } catch (err) {
      console.error("Error:", err);
      setPremium("Error");
      setOpen(true);
    }

    setLoading(false);
  };

  return (
    <Box>
      <LifeAssuredDetail fields={fields} setFields={setFields} />
      <PlanAndCoverages fields={fields} setFields={setFields} />

      <Box textAlign="center" marginTop={3}>
        <Button variant="contained" onClick={calculatePremium} disabled={loading}>
          {loading ? <CircularProgress size={26} /> : "Calculate Premium"}
        </Button>
      </Box>

      <Snackbar open={open} autoHideDuration={3000} onClose={() => setOpen(false)}>
        <Alert severity="success">Premium: {premium}</Alert>
      </Snackbar>
    </Box>
  );
}


